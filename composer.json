{
  "name": "blezigen/laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Dubrovin",
      "email": "rdubrovin@ronasit.com"
    },
    {
      "name": "Blezigen",
      "email": "khairullin.it@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "laravel/framework": ">=5.3.0",
    "minime/annotations": "dev-master",
    "spatie/data-transfer-object": "^2.8"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Support\\AutoDoc\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  }
}
